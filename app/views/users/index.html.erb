<table class="table table-striped">
	<thead>
		<th> Email </th>
		<th> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp; </th>
	</thead>
	<tbody>
		<% @users.each do |user| %>
			<tr>
				<td> <%= user.email %></td> 
				<td><%= link_to 'Edit', edit_user_path(user.id) %> | 
					<%= link_to 'Delete', user_path(user), method: :delete, data: {confirm: true} %> </td>
			</tr>
		<% end %>
	</tbody>
</table>